{
	"imports": {
		"@std/fmt": "jsr:@std/fmt@^1.0.7",
		"@std/path": "jsr:@std/path@^1.0.9",
		"react": "npm:react",
		"react-dom": "npm:react-dom",
		"react-router": "npm:react-router",
		"isbot": "npm:isbot"
	},
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"unstable": ["kv", "temporal", "worker-options"],
	"nodeModulesDir": "auto",
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"semiColons": true,
		"singleQuote": true,
		"proseWrap": "preserve"
	}
}
